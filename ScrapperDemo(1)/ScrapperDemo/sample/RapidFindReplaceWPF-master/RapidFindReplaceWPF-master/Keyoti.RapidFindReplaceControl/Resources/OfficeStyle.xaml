        <Setter Property="BodyIterativeHighlightAdornerBrush" Value="{StaticResource Keyoti_BodyIterativeHighlightAdornerBrush}"/>
        <Setter Property="NoHitsBorderBrushColor" Value="#FFFB1515"/>
        <Setter Property="WatermarkText" Value="Find..."/>
        <Setter Property="HorizontalAlignment" Value="Center" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="FindButtonIcon" Value="{StaticResource Keyoti_MagnifyingGlassIconBrush}"/>
        <Setter Property="OptionsButtonIcon" Value="{StaticResource Keyoti_OptionsIconBrush}"/>
        <Setter Property="OptionsButtonCheckedIcon" Value="{StaticResource Keyoti_OptionsCheckedIconBrush}"/>
        <Setter Property="IsReplaceOpen" Value="false"/>
        <Setter Property="FocusManager.IsFocusScope" Value="True"/>
        <Setter Property="KeyboardNavigation.TabNavigation" Value="Local"/>
        <Setter Property="KeyboardNavigation.IsTabStop" Value="false"/>
        <Setter Property="Validation.ErrorTemplate" Value="{StaticResource Keyoti_QueryErrorTemplate}"/>

        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type local:RapidFindReplaceControl}" >


                    <Grid HorizontalAlignment="Stretch" d:DesignHeight="130" d:DesignWidth="500">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="MouseStates">
                                <VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <ColorAnimation Storyboard.TargetProperty="(TextBlock.Foreground).(Color)" Storyboard.TargetName="PART_EmptyText" Duration="0:0:0.1" To="Black"/>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="MouseOut">
                                    <Storyboard>
                                        <ColorAnimation Storyboard.TargetProperty="(TextBlock.Foreground).(Color)" Storyboard.TargetName="PART_EmptyText" Duration="0:0:0.1" To="Gray"/>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="QueryStates">
                                <VisualState x:Name="Query">
                                    <Storyboard>
                                        <StringAnimationUsingKeyFrames Storyboard.TargetProperty="(TextBlock.Text)" Storyboard.TargetName="PART_EmptyText" Duration="0" >
                                            <DiscreteStringKeyFrame Value="" KeyTime="0"/>
                                        </StringAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="NoQuery"/>
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="FocusStates">
                                <VisualState x:Name="Blur_Query"/>
                                <VisualState x:Name="Blur_NoQuery">
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="PART_EmptyText" Duration="0:0:0.2"  To="1"/>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Focus">
                                    <Storyboard>
                                        <StringAnimationUsingKeyFrames Storyboard.TargetProperty="(TextBlock.Text)" Storyboard.TargetName="PART_EmptyText" Duration="0" >
                                            <DiscreteStringKeyFrame Value="" KeyTime="0"/>
                                        </StringAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="HitStates">
                                <VisualState x:Name="NoHits">
                                    <Storyboard>
                                        <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)" Storyboard.TargetName="mainBorder" Duration="0:0:0.25">
                                            <EasingColorKeyFrame KeyTime="0" Value="Transparent"/>
                                            <EasingColorKeyFrame KeyTime="0:0:.25" Value="#FFFB1515" x:Name="PART_EasingColorKeyFrameEnd" />

                                        </ColorAnimationUsingKeyFrames>


                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(Control.Background)" Storyboard.TargetName="PART_FindButton"  Duration="0:0:0">
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{StaticResource Keyoti_CrossIconBrush}"/>
                                        </ObjectAnimationUsingKeyFrames>



                                    </Storyboard>
                                </VisualState>

                                <VisualState x:Name="Hits">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(Control.Background)" Storyboard.TargetName="PART_FindButton"  Duration="0:0:0">
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{StaticResource Keyoti_CrossIconBrush}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <BooleanAnimationUsingKeyFrames Storyboard.TargetProperty="(IsEnabled)" Storyboard.TargetName="PART_NextMatchButton" Duration="0">
                                            <DiscreteBooleanKeyFrame Value="True" KeyTime="0"/>
                                        </BooleanAnimationUsingKeyFrames>
                                        <BooleanAnimationUsingKeyFrames Storyboard.TargetProperty="(IsEnabled)" Storyboard.TargetName="PART_PreviousMatchButton" Duration="0">
                                            <DiscreteBooleanKeyFrame Value="True" KeyTime="0"/>
                                        </BooleanAnimationUsingKeyFrames>
                                        <BooleanAnimationUsingKeyFrames Storyboard.TargetProperty="(IsEnabled)" Storyboard.TargetName="PART_ReplaceButton" Duration="0">
                                            <DiscreteBooleanKeyFrame Value="True" KeyTime="0"/>
                                        </BooleanAnimationUsingKeyFrames>
                                        <BooleanAnimationUsingKeyFrames Storyboard.TargetProperty="(IsEnabled)" Storyboard.TargetName="PART_ReplaceAllButton" Duration="0">
                                            <DiscreteBooleanKeyFrame Value="True" KeyTime="0"/>
                                        </BooleanAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="NoFind">
                                    <Storyboard>
                                        <ColorAnimation Storyboard.TargetProperty="(TextBlock.Foreground).(Color)" Storyboard.TargetName="PART_EmptyText" Duration="0:0:0.1" To="Gray"/>
                                        <BooleanAnimationUsingKeyFrames Storyboard.TargetProperty="(IsEnabled)" Storyboard.TargetName="PART_ReplaceButton" Duration="0">
                                            <DiscreteBooleanKeyFrame Value="False" KeyTime="0"/>
                                        </BooleanAnimationUsingKeyFrames>
                                        <BooleanAnimationUsingKeyFrames Storyboard.TargetProperty="(IsEnabled)" Storyboard.TargetName="PART_ReplaceAllButton" Duration="0">
                                            <DiscreteBooleanKeyFrame Value="False" KeyTime="0"/>
                                        </BooleanAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>


                        <Grid  HorizontalAlignment="Stretch" Margin="0,0,0,10" >



