                                                <ColumnDefinition Width="26.222"/>
                                                <ColumnDefinition Width="16"/>
                                            </Grid.ColumnDefinitions>

                                            <TextBox Grid.Column="0" Padding="1" Margin="0,0,0,2" x:Name="PART_FindTextBox" 
    														BorderThickness="1" BorderBrush="#FF888888"
    														HorizontalAlignment="Stretch"
    														local:RapidFindReplaceControl.IsFindable="false" 
    														FontSize="10pt" TabIndex="1"
    														VerticalContentAlignment="Top" Grid.ColumnSpan="4"
													
                                                    
    														Text="{Binding Query, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}, UpdateSourceTrigger=PropertyChanged}" 
										 
													
                                                    
                                         >
                                                <TextBox.Background>

                                                    <VisualBrush Stretch="None" AlignmentX="Left">

                                                        <VisualBrush.Transform>
                                                            <TranslateTransform X="5" Y="-.6"/>
                                                        </VisualBrush.Transform>
                                                        <VisualBrush.Visual>
                                                            <TextBlock  x:Name="PART_EmptyText" Grid.Column="0" Text="{TemplateBinding WatermarkText}" Foreground="Gray" FontSize="{Binding FontSize, ElementName=PART_FindTextBox, Mode=OneWay}" Opacity="0" Focusable="False"/>
                                                        </VisualBrush.Visual>
                                                    </VisualBrush>

                                                </TextBox.Background>
                                            </TextBox>

                                            <local:PinnedPopup  x:Name="PART_OptionsPopup" AllowsTransparency="true" StaysOpen="false"
    														PlacementTarget="{Binding ElementName=mainBorder}" Placement="Custom" 
                                                   
    														IsOpen="{Binding IsOptionsDropDownOpen, RelativeSource={RelativeSource TemplatedParent}}" Grid.ColumnSpan="2" Margin="0,0,3.778,0" >




                                                <Grid  Background="{StaticResource Keyoti_BackgroundBrush}" Margin="0 0 8 8" x:Name="popUpGrid">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition />
                                                        <RowDefinition />
                                                        <RowDefinition />
                                                        <RowDefinition Height="*"/>
                                                    </Grid.RowDefinitions>


                                                    <Rectangle Grid.RowSpan="4" Grid.ColumnSpan="1" Fill="{Binding Background, ElementName=popUpGrid}" Margin="0 0 0 0" Stroke="#FFAAAAAA">
                                                        <Rectangle.Effect>
                                                            <DropShadowEffect Color="#FF000000" Opacity="0.5" RenderingBias="Performance"  />
                                                        </Rectangle.Effect>
                                                    </Rectangle>

                                                    <ListView Grid.Row="0"  x:Name="PART_HistoryList" Background="Transparent" BorderThickness="0" Padding="0" Margin="0" 
    																SelectedItem="{Binding Query, Mode=OneWayToSource, RelativeSource={RelativeSource TemplatedParent}, UpdateSourceTrigger=PropertyChanged}"
    																ItemsSource="{TemplateBinding QueryHistory}"
                                              
                                                  >



                                                        <ListView.Resources>
                                                            <Style TargetType="{x:Type GridViewColumnHeader}">
                                                                <Setter Property="Visibility" Value="Collapsed" />
                                                            </Style>
                                                        </ListView.Resources>
                                                        <ListView.ItemContainerStyle>
                                                            <StaticResource ResourceKey="Keyoti_ListViewItemContainerStyle1"/>
                                                        </ListView.ItemContainerStyle>
                                                        <ListView.View>
                                                            <GridView   >
                                                                <GridViewColumn Width="200"/>

                                                            </GridView>

                                                        </ListView.View>


                                                    </ListView>



                                                </Grid>




                                            </local:PinnedPopup>


                                            <Button Grid.Row="0" Grid.Column="1" x:Name="PART_FindButton"  Width="{Binding ActualHeight, ElementName=PART_FindTextBox}" Height="{Binding ActualHeight, ElementName=PART_FindTextBox}" 
    														Background="{Binding FindButtonIcon, RelativeSource={RelativeSource TemplatedParent}}" Grid.ColumnSpan="2"
    														TabIndex="3" Margin="0 1 0 3"
                                                >
                                                <Button.Template>
                                                    <ControlTemplate TargetType="{x:Type Button}">
                                                        <Border x:Name="Border"  BorderBrush="Transparent">

                                                            <ContentPresenter>
                                                                <ContentPresenter.Content>
                                                                    <Rectangle Fill="{Binding Background, RelativeSource={RelativeSource TemplatedParent}}"/>
                                                                </ContentPresenter.Content>
                                                            </ContentPresenter>
                                                        </Border>
                                                        <ControlTemplate.Triggers>

                                                            <Trigger Property="IsMouseOver" Value="true">
                                                                <Setter TargetName="Border" Property="Background" Value="{StaticResource Keyoti_FindButtonBackgroundMouseOverBrush}" />
                                                            </Trigger>
                                                            <Trigger Property="IsPressed" Value="true">
                                                                <Setter Property="Background" TargetName="Border" Value="{DynamicResource {x:Static SystemColors.HighlightBrushKey}}"/>
                                                            </Trigger>

                                                        </ControlTemplate.Triggers>
                                                    </ControlTemplate>
                                                </Button.Template>
                                            </Button>

                                            <ToggleButton IsChecked="{Binding IsOptionsDropDownOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}" Grid.Column="3" x:Name="PART_OptionsButton" Width="16"  Height="{Binding ActualHeight, ElementName=PART_FindTextBox}"  
    														Background="{Binding OptionsButtonIcon, RelativeSource={RelativeSource TemplatedParent}}"
    														TabIndex="4"  Margin="0 1 1 3"
                                                >
                                                <ToggleButton.Template>
                                                    <ControlTemplate TargetType="{x:Type ToggleButton}">
                                                        <Border x:Name="Border"  BorderBrush="Transparent" BorderThickness=".75,0,0,0">

                                                            <ContentPresenter>
                                                                <ContentPresenter.Content>
                                                                    <Rectangle x:Name="rect" Fill="{Binding Background, RelativeSource={RelativeSource TemplatedParent}}"/>
                                                                </ContentPresenter.Content>
                                                            </ContentPresenter>
                                                        </Border>
                                                        <ControlTemplate.Triggers>

                                                            <MultiTrigger>
                                                                <MultiTrigger.Conditions>
                                                                    <Condition Property="IsMouseOver" Value="true" />
                                                                    <Condition Property="IsChecked" Value="false" />
                                                                </MultiTrigger.Conditions>

                                                                <Setter TargetName="Border" Property="Background" Value="{StaticResource Keyoti_OptionsButtonBackgroundMouseOverBrush}" />
                                                                <Setter TargetName="Border" Property="BorderThickness" Value=".75,0,0,0" />
                                                                <Setter TargetName="Border" Property="BorderBrush" Value="#FF406CB9" />

                                                            </MultiTrigger>

                                                            <Trigger Property="IsChecked" Value="true">
                                                                <Setter TargetName="Border" Property="Background" Value="#FF406CB9" />
                                                                <Setter TargetName="rect" Property="Fill" Value="{StaticResource Keyoti_OptionsCheckedIconBrush}" />

                                                            </Trigger>
                                                            <Trigger Property="IsPressed" Value="true">
                                                                <Setter Property="Background" TargetName="Border" Value="{DynamicResource {x:Static SystemColors.HighlightBrushKey}}"/>
                                                            </Trigger>


                                                        </ControlTemplate.Triggers>
                                                    </ControlTemplate>
                                                </ToggleButton.Template>
                                            </ToggleButton>





                                        </Grid>
                                    </Border>
                                    <Border Grid.Column="1" Grid.Row="1" x:Name="replaceBorder" BorderBrush="Transparent" BorderThickness="1" Height="28" VerticalAlignment="Top"
												Visibility="{TemplateBinding IsReplaceOpen, Converter={StaticResource Keyoti_BooleanCollapsedVisibilityConverter}}"
    												>
                                        <TextBox x:Name="PART_ReplaceTextBox" BorderThickness="1" Padding="1"  MinWidth="50"  
    													local:RapidFindReplaceControl.IsFindable="false" BorderBrush="#FF888888" Margin="0,0,-0.333,2" 
    													FontSize="10pt"  TabIndex="2"
    													Width="Auto" Height="Auto"
    													Background="{StaticResource Keyoti_FindBoxBackgroundBrush}"
								
						 	/>

                                    </Border>





                                    <Grid Grid.Column="0" Grid.Row="2" Grid.ColumnSpan="2"  VerticalAlignment="Bottom">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="90"/>
                                            <ColumnDefinition Width="90"/>
                                            <ColumnDefinition Width="90"/>
                                            <ColumnDefinition Width="90"/>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="24"/>
                                            <RowDefinition Height="*"/>
                                        </Grid.RowDefinitions>


                                        <ToggleButton Grid.Column="0" Margin="4 0 0 0" x:Name="More" Width="60" HorizontalAlignment="Left" VerticalContentAlignment="Center"
    														TabIndex="5"    >
                                            <ToggleButton.Style>
                                                <Style TargetType="{x:Type ToggleButton}">
                                                    <Setter Property="Content" Value="More &gt;&gt;"/>
                                                    <Style.Triggers>
                                                        <Trigger Property="IsChecked" Value="True">
                                                            <Setter Property="Content">
                                                                <Setter.Value>
                                                                    <TextBlock Text="&lt;&lt;Less"/>
                                                                </Setter.Value>
                                                            </Setter>
                                                        </Trigger>
                                                        <Trigger Property="IsChecked" Value="False">
                                                            <Setter Property="Content">
                                                                <Setter.Value>
                                                                    <TextBlock Text="More &gt;&gt;"/>
                                                                </Setter.Value>
                                                            </Setter>
                                                        </Trigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </ToggleButton.Style>
                                        </ToggleButton>


                                        <Button Grid.Column="1"   x:Name="PART_ReplaceButton" Margin="2 0 2 0"
    														Visibility="{TemplateBinding IsReplaceOpen, Converter={StaticResource Keyoti_BooleanCollapsedVisibilityConverter}}"
                        
    														TabIndex="7"                                
											>Replace
                                            <Button.IsEnabled>
                                                <MultiBinding Converter="{StaticResource Keyoti_BooleanAndConverter}">
                                                    <Binding Path="CurrentMatch.IsReplaceable" RelativeSource="{RelativeSource TemplatedParent}"/>
                                                    <Binding Path="IsEnabled"/>
                                                </MultiBinding>
                                            </Button.IsEnabled>


                                        </Button>
                                        <Button Grid.Column="2"   x:Name="PART_ReplaceAllButton" Margin="2 0 2 0"
    														Visibility="{TemplateBinding IsReplaceOpen, Converter={StaticResource Keyoti_BooleanCollapsedVisibilityConverter}}"
    														TabIndex="8"                               
											>Replace All
                                            <Button.IsEnabled>
                                                <MultiBinding Converter="{StaticResource Keyoti_BooleanAndConverter}">
                                                    <Binding Path="CurrentMatch.IsReplaceable" RelativeSource="{RelativeSource TemplatedParent}"/>
                                                    <Binding Path="IsEnabled"/>
                                                </MultiBinding>
                                            </Button.IsEnabled>

                                        </Button>


                                        <Button Grid.Column="3" x:Name="PART_NextMatchButton" VerticalContentAlignment="Center" Margin="2 0 2 0"
    														TabIndex="5"   IsEnabled="False" >Find Next

                                        </Button>



                                        <Button Grid.Column="4" x:Name="PART_PreviousMatchButton" Margin="2 0 2 0"
    														TabIndex="6"  IsEnabled="False" 
										>
                                            Find Previous
                                        </Button>


                                        <Grid Grid.Row="1" Grid.ColumnSpan="5" Visibility="{Binding ElementName=More, Path=IsChecked, Converter={StaticResource Keyoti_BooleanCollapsedVisibilityConverter}}">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="28"/>
                                                <RowDefinition Height="*"/>
                                            </Grid.RowDefinitions>

                                            <Label Grid.Row="0" Foreground="#FF707070" Content="Find options"/>
                                            <ListView Grid.Row="1" x:Name="PART_OptionsList" Background="Transparent" BorderThickness="0" Padding="0" 
																	HorizontalAlignment="Stretch" VerticalAlignment="Stretch" 
    																ItemsSource="{Binding AvailableFindOptions, RelativeSource={RelativeSource TemplatedParent}}" >

                                                <ListView.Resources>
                                                    <Style TargetType="{x:Type GridViewColumnHeader}">
                                                        <Setter Property="Visibility" Value="Collapsed" />
                                                    </Style>
                                                    <Style TargetType="{x:Type CheckBox}">

                                                        <Setter Property="HorizontalAlignment" Value="Right"/>
                                                    </Style>
                                                </ListView.Resources>
                                                <ListView.ItemContainerStyle>
                                                    <StaticResource ResourceKey="Keyoti_ListViewItemContainerStyle1"/>

                                                </ListView.ItemContainerStyle>
                                                <ListView.View>
                                                    <GridView    >
                                                        <GridViewColumn Width="30">
                                                            <GridViewColumn.CellTemplate>
                                                                <DataTemplate>

                                                                    <CheckBox IsHitTestVisible="False" HorizontalAlignment="Right" Tag="{Binding OptionName}" IsChecked="{Binding Value}" Padding="0" Margin="0"/>

                                                                </DataTemplate>

                                                            </GridViewColumn.CellTemplate>

                                                        </GridViewColumn>
                                                        <GridViewColumn DisplayMemberBinding="{Binding OptionName, Converter={StaticResource Keyoti_CamelCaseConverter}, Mode=OneWay}" Width="180"/>

                                                    </GridView>

                                                </ListView.View>

                                                <ListView.ItemsPanel>
                                                    <ItemsPanelTemplate>
                                                        <WrapPanel Width="{Binding (FrameworkElement.ActualWidth), RelativeSource={RelativeSource AncestorType=ScrollContentPresenter}}"
																			   ItemWidth="{Binding (ListView.View).ItemWidth, RelativeSource={RelativeSource AncestorType=ListView}}"
																			   MinWidth="{Binding ItemWidth, RelativeSource={RelativeSource Self}}"
																			   ItemHeight="{Binding (ListView.View).ItemHeight, RelativeSource={RelativeSource AncestorType=ListView}}" />
                                                    </ItemsPanelTemplate>
                                                </ListView.ItemsPanel>
                                            </ListView>
                                        </Grid>
                                    </Grid>



                                </Grid>

                            </Grid>




                        </Grid>







                    </Grid>



                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Margin" Value="0,0,-6,0"/>
    </Style>









   
  

            
</ResourceDictionary>